{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb",
    "airbnb/hooks"
  ],
  "parser": "babel-eslint",
  "plugins": ["react", "react-hooks", "promise"],
  "rules": {
    "react/jsx-props-no-spreading": 0,
    "import/no-named-as-default": 0
  }
}